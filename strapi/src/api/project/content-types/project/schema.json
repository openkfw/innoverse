{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 60
    },
    "featured": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "description": "Please choose an image that is not transparent. It might not provide the necessary contrast on the page.",
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Exploration",
        "Konzeption",
        "Proof of Concept",
        "Live"
      ],
      "required": true,
      "default": "Exploration"
    },
    "summary": {
      "type": "text",
      "required": true
    },
    "description": {
      "displayName": "Description",
      "type": "component",
      "repeatable": false,
      "component": "description.description",
      "required": true
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::inno-user.inno-user"
    },
    "projectStart": {
      "type": "date",
      "required": true
    },
    "shortTitle": {
      "type": "string",
      "maxLength": 30
    },
    "team": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inno-user.inno-user",
      "mappedBy": "project"
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "project"
    }
  }
}
